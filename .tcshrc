fetch

set     red="%{\033[1;31m%}"
set   green="%{\033[0;32m%}"
set  yellow="%{\033[1;33m%}"
set    blue="%{\033[1;34m%}"
set magenta="%{\033[1;35m%}"
set    cyan="%{\033[1;36m%}"
set   white="%{\033[0;37m%}"
set     end="%{\033[0m%}" 

set path = ( /bin /usr/local/bin usr/bin )

set autoexpand
set autolist
set noclobber

alias l 'ls -gA'
alias vim nvim
set prompt = "${red}[${yellow}%n${green}@${blue}%m ${magenta}%c${red}]${white}$ ${end}"


alias ls ls --color=always
alias cl clear

unset red green yellow blue magenta cyan yellow white end
